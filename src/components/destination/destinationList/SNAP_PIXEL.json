[
  {
    "title": "Connection Settings",
    "fields": [
      {
        "type": "textInput",
        "label": "Pixel ID",
        "value": "pixelId",
        "inputFieldType": "password",
        "regex": "^(.{0,100})$",
        "regexErrorMessage": "Invalid Pixel ID",
        "required": true,
        "placeholder": "e.g: 836d6231-aad6-4e41-9cbb-5854565cad8e",
        "secret": true,
        "footerNote": "Your Pixel ID"
      },
      {
        "type": "singleSelect",
        "label": "Hashing Method",
        "value": "hashMethod",
        "placeholder": "NONE",
        "options": [
          {
            "name": "NONE",
            "value": "none"
          },
          {
            "name": "SHA-256",
            "value": "sha256"
          }
        ],
        "defaultOption": {
          "name": "NONE",
          "value": "none"
        }
      }
    ]
  },
  {
    "title": "Native SDK",
    "fields": [
      {
        "type": "defaultCheckbox",
        "label": "Use device-mode to send events",
        "value": "useNativeSDK",
        "default": true
      }
    ]
  }
]
